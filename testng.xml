<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="通用矿版">
    <parameter name="driver" value="FirefoxDriver" />
    <test verbose="2" preserve-order="true" name="查询功能">
        <classes>
            <class name="com.webui.tests.ReportDangerListPageTests">
                <methods>
                    <include name="TC_searchByAddressCate"/>
                    <exclude name="TC_searchByYeMhazardDesc"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="关联风险点">
        <classes>
            <class name="com.webui.tests.ReportDangerListPageTests">
                <methods>
                    <include name="TC_chooseAddress"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>